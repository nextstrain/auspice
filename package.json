    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.2.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "jest": "^25.1.0",
    "jest-circus": "^25.3.0",
